; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Asteroids"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1200
window/size/viewport_height=900
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="canvas_items"
window/stretch/scale=1.33

[editor]

naming/node_name_num_separator=2
naming/scene_name_casing=1
naming/script_name_casing=1

[layer_names]

2d_render/layer_1="Background"
2d_render/layer_2="Rocks"
2d_render/layer_3="Player"
2d_render/layer_4="Effects"
2d_render/layer_5="UI"
2d_physics/layer_1="Player"
2d_physics/layer_2="Rocks"
2d_physics/layer_3="Lazers"

[physics]

2d/default_gravity_vector=Vector2(0, 0)
2d/default_linear_damp=15.0
2d/default_angular_damp=0.0
common/physics_interpolation=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
viewport/hdr_2d=true
